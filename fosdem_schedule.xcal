      <pentabarf:title>The case against "It just works" or how to avoid #idiocracy</pentabarf:title>
      <pentabarf:subtitle>Design, education, politics and fake news.</pentabarf:subtitle>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
      <pentabarf:track>Open Source Design</pentabarf:track>
