      <pentabarf:title>Mozilla's DeepSpeech and Common Voice projects</pentabarf:title>
      <pentabarf:subtitle>Open and offline-capable voice recognition for everyone</pentabarf:subtitle>
      <pentabarf:language>en</pentabarf:language>
