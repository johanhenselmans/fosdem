      <pentabarf:title>De-mystifying contributing to PostgreSQL</pentabarf:title>
      <pentabarf:subtitle/>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
      <pentabarf:track>PostgreSQL</pentabarf:track>
      <pentabarf:start>2018-02-03 11:00:00 +0100</pentabarf:start>
