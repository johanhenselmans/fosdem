      <pentabarf:title>... like real computers!</pentabarf:title>
      <pentabarf:subtitle>Making distributions work on single board computers</pentabarf:subtitle>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
