      <pentabarf:title>Whisper and Swarm Protocol for RTC</pentabarf:title>
      <pentabarf:subtitle>Decentralised, Distributed and Deniable, Communication.</pentabarf:subtitle>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
      <pentabarf:track>Real Time Communications</pentabarf:track>
      <pentabarf:start>2018-02-04 14:10:00 +0100</pentabarf:start>
