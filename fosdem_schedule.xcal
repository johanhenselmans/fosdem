      <pentabarf:title>Building an integration testing framework</pentabarf:title>
      <pentabarf:subtitle>Integration testing framework we are building to test GitLab using Docker and Kubernetes</pentabarf:subtitle>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
