      <pentabarf:title>GStreamer &amp; Rust</pentabarf:title>
      <pentabarf:subtitle>Fast, safe and productive multimedia software</pentabarf:subtitle>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
      <pentabarf:track>Rust</pentabarf:track>
      <pentabarf:start>2018-02-04 11:00:00 +0100</pentabarf:start>
