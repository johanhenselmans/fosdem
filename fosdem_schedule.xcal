      <pentabarf:title>Why you should take a look at Rust?</pentabarf:title>
      <pentabarf:subtitle>Insights into a modern, safe, fast language</pentabarf:subtitle>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
      <pentabarf:track>Rust</pentabarf:track>
      <pentabarf:start>2018-02-04 09:00:00 +0100</pentabarf:start>
