      <pentabarf:title>Servers can't be trusted, and thanks to tamper-proof journals EteSync doesn't need to!</pentabarf:title>
      <pentabarf:subtitle/>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
      <pentabarf:track>Decentralised Internet and Privacy</pentabarf:track>
      <pentabarf:start>2018-02-04 16:30:00 +0100</pentabarf:start>
