      <pentabarf:title>Graph-based analysis of JavaScript repositories</pentabarf:title>
      <pentabarf:subtitle>Incremental static analysis of JavaScript source code defined by declarative queries</pentabarf:subtitle>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
      <pentabarf:track>Source Code Analysis</pentabarf:track>
      <pentabarf:start>2018-02-04 16:20:00 +0100</pentabarf:start>
