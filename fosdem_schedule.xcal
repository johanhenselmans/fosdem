      <pentabarf:title>Componolit - a component-based open-source platform for trustworthy mobile devices</pentabarf:title>
      <pentabarf:subtitle>Deprivileging the Google software stack.</pentabarf:subtitle>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
      <pentabarf:track>Embedded, mobile and automotive</pentabarf:track>
      <pentabarf:start>2018-02-03 14:30:00 +0100</pentabarf:start>
