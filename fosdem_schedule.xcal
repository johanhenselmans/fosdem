      <pentabarf:title>Babelfish: a universal code parser for source code analysis</pentabarf:title>
      <pentabarf:subtitle/>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
      <pentabarf:track>Source Code Analysis</pentabarf:track>
      <pentabarf:start>2018-02-04 12:40:00 +0100</pentabarf:start>
