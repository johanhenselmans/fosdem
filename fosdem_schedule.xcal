      <pentabarf:title>The half rolling repository model</pentabarf:title>
      <pentabarf:subtitle>The golden intersection for desktop users?</pentabarf:subtitle>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
      <pentabarf:track>Distributions</pentabarf:track>
      <pentabarf:start>2018-02-04 16:30:00 +0100</pentabarf:start>
